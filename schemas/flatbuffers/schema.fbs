namespace fbuf;

attribute "priority";

table Head {
  tid:long;
  ts:long;
  file:string;
  line:long;
}

table Dot {
  h:Head;
  src:string;
}

enum Compression : byte {zstd = 1, lz4 = 2}

table Table {
  h:Head;
  data:[ubyte];
}

union Any { Dot, Table }

table Document {
  items:[Any];
}


root_type Document;